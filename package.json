{
  "name": "kanban",
  "version": "0.0.1",
  "description": "KanBan",
  "scripts": {
    "precommit": "npm test && npm run build",
    "start": "./node_modules/.bin/webpack-dev-server --inline --content-base build/ -d",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require tests/setup.js --require babel-register --require ignore-styles",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm test",
    "build": "./node_modules/.bin/webpack -p",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "app/**/*.spec.js"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LucasBassetti/kanban"
  },
  "keywords": [
    "movie",
    "collection"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasBassetti/kanban/issues"
  },
  "homepage": "https://github.com/LucasBassetti/kanban#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nyc": "^10.2.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.5.0",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.11.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "random-id": "0.0.2",
    "react": "^15.5.4",
    "react-addons-update": "^15.6.0",
    "react-dd-menu": "^2.0.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "uuid": "^3.1.0"
  }
}
